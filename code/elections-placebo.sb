#!/bin/bash
#SBATCH -A dku139
#SBATCH --job-name="elections-placebo-fixed"
#SBATCH --output="elections-placebo-fixed.%j.%N.out"
#SBATCH --partition gpu-shared
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=6
#SBATCH --gres=gpu:3
#SBATCH --mem=50GB
#SBATCH -t 48:00:00

#Load the cuda module
source ../venv/bin/activate
module load cuda/10.1
module load R/3.6.1
nvcc -V
nvidia-smi

#Run the job
./code/elections-placebo.sh